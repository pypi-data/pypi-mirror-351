# ETDI Auth0 Configuration
# Copy this file to .env and fill in your actual values

# Auth0 Domain (e.g., your-tenant.auth0.com)
ETDI_AUTH0_DOMAIN=your-auth0-domain.auth0.com

# Auth0 Client ID for ETDI Tool Provider
ETDI_CLIENT_ID=your-auth0-client-id

# Demo mode (set to 'true' for demonstrations)
ETDI_DEMO_MODE=true

# Verbose logging (set to 'true' for detailed logs)
ETDI_VERBOSE=true
