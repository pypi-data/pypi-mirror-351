{
  "name": "maap-dps-jupyter-extension",
  "version": "0.0.00000",
  "description": "A JupyterLab extension for submitting and viewing jobs.",
  "keywords": [
    "dps",
    "jobs",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/MAAP-Project/dps-jupyter-extension",
  "bugs": {
    "url": "https://github.com/MAAP-Project/dps-jupyter-extension.git/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Marjorie Lucas",
    "email": "marjorie.j.lucas@jpl.nasa.gov"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/MAAP-Project/dps-jupyter-extension.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev",
    "build:rollup": "rollup -c",
    "build:prod": "jlpm clean && jlpm build:lib && jlpm build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf maap_dps_jupyter_extension/labextension",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension",
    "install:extension": "jlpm build",
    "start": "jupyter lab --config tests/jupyter_server_test_config.py",
    "test": "jlpm playwright test",
    "test:ui": "jlpm playwright test --ui",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.1.8",
    "@jupyterlab/apputils": "^4.2.8",
    "@jupyterlab/coreutils": "^6.1.8",
    "@jupyterlab/galata": "^5.2.0",
    "@jupyterlab/launcher": "^4.1.8",
    "@jupyterlab/mainmenu": "^4.1.8",
    "@jupyterlab/ui-components": "^4.1.8",
    "@playwright/test": "^1.44.0",
    "@reduxjs/toolkit": "1.9.6",
    "@types/react": "^18.3.1",
    "bootstrap": "5.2.0",
    "match-sorter": "^4.0.0",
    "react": "^18.3.1",
    "react-bootstrap": "2.4.0",
    "react-bootstrap-icons": "^1.8.4",
    "react-dom": "^18.3.1",
    "react-icons": "^4.4.0",
    "react-json-view": "^1.21.3",
    "react-paginate": "^8.1.3",
    "react-redux": "^8.0.2",
    "react-select": "^5.4.0",
    "react-table": "^7.8.0",
    "split-pane-react": "^0.1.3"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.1.8",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/match-sorter": "^6.0.0",
    "@types/react-dom": "^18.0.6",
    "@types/react-table": "7.7.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^4.6.1",
    "typescript": "~5.4.5"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "maap_dps_jupyter_extension/labextension"
  },
  "jupyter-releaser": {
    "hooks": {
      "before-build-npm": [
        "python -m pip install jupyterlab~=4.3",
        "jlpm"
      ],
      "before-build-python": [
        "jlpm clean:all"
      ]
    }
  }
}
