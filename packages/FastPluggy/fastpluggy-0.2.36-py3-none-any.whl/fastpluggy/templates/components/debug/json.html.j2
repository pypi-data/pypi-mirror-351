{% from "components/common.html.j2" import render_card_header %}

<div class="card">
    {{ render_card_header(view) }}
    <div id="collapse-{{ view.id }}" class="collapse {% if not view.collapsed %}show{% endif %}">
        <div class="card-body">
            <pre class="debug-data">
{{ view.json_data | escape}}
