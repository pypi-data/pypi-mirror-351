{
  "mechanism_plot": {
    "default": {
      "marker": "o",
      "color": "maroon",
      "zorder": 4
    },
    "ground": {
      "ls": "--",
      "color": "black",
      "zorder": 3
    },
    "dotted": {
      "ls": ":",
      "color": "black",
      "zorder": 3
    }
  },
  "joint_path": {
    "default": {
      "color": "deepskyblue",
      "zorder": 2
    }
  },
  "vel_arrow": {
    "arrowstyle": "-|>",
    "shrinkA": 0,
    "shrinkB": 0,
    "mutation_scale": 20,
    "lw": 1.5,
    "zorder": 5,
    "facecolor": "darkgrey",
    "edgecolor": "darkgrey"
  },
  "acc_arrow": {
    "arrowstyle": "-|>",
    "shrinkA": 0,
    "shrinkB": 0,
    "mutation_scale": 20,
    "lw": 1.5,
    "zorder": 5,
    "facecolor": "orange",
    "edgecolor": "orange"
  },
  "cam_plot": {
    "default": {
      "color": "maroon"
    },
    "naive_line": {
      "color": "maroon",
      "ls": "-",
      "label": "Naive"
    },
    "harmonic_line": {
      "color": "deepskyblue",
      "ls": "--",
      "alpha": 0.85,
      "label": "Harmonic"
    },
    "cycloidal_line": {
      "color": "black",
      "ls": "-.",
      "alpha": 0.7,
      "label": "Cycloidal"
    },
    "pitch_line": {
      "color": "deepskyblue",
      "ls": ":",
      "label": "Pitch"
    },
    "base_circle": {
      "color": "black",
      "ls": "--",
      "label": "Base"
    },
    "fill": {
      "color": "maroon",
      "ec": "black",
      "lw": 2,
      "zorder": 2
    },
    "follower_fill": {
      "color": "darkgrey",
      "ec": "black",
      "lw": 2,
      "zorder": 2
    }
  },
  "gear_plot": {
    "tooth": {
      "color": "maroon",
      "zorder": 3
    },
    "pitch": {
      "color": "black",
      "ls": "--",
      "zorder": 2,
      "label": "Pitch"
    },
    "base": {
      "color": "deepskyblue",
      "ls": "--",
      "zorder": 2,
      "label": "Base"
    }
  }
}