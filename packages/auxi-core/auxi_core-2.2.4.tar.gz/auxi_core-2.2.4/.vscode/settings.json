{
    "files.exclude": {
        ".mypy_cache": true,
        ".pytest_cache": true,
        ".ruff_cache": true,
        ".vscode": true,
        "build": true,
        "dist": true,
        "htmlcov": true,
        ".coverage": true,
        ".gitignore": true,
        ".gitlab-ci.yml": true,
        ".gitlab-ci-local": true,
        "Pipfile.lock": true,
        ".pre-commit-config.yaml": true,
        "typings": true,
        ".venv": true,
        "**/__pycache__": true,
        "**/.directory": true,
        "**/*.egg-info": true
    },
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
}
