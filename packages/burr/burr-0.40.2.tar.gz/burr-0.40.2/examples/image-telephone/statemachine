digraph {
	graph [compound=false concentrate=false rankdir=TB ranksep=0.4]
	caption [label=caption shape=box style=rounded]
	analyze [label=analyze shape=box style=rounded]
	generate [label=generate shape=box style=rounded]
	terminal [label=terminal shape=box style=rounded]
	caption -> analyze [style=solid]
	analyze -> terminal [label="len(image_caption_history) == 4" style=dashed]
	analyze -> generate [style=solid]
	generate -> caption [style=solid]
}
