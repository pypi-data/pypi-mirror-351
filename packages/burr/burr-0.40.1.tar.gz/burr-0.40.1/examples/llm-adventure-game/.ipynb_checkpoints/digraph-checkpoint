digraph {
	graph [compound=false concentrate=false rankdir=TB ranksep=0.4]
	start [label=start shape=box style=rounded]
	prompt_for_challenge [label=prompt_for_challenge shape=box style=rounded]
	evaluate_attempt [label=evaluate_attempt shape=box style=rounded]
	maybe_progress [label=maybe_progress shape=box style=rounded]
	win [label=win shape=box style=rounded]
	start -> prompt_for_challenge [style=solid]
	prompt_for_challenge -> evaluate_attempt [style=solid]
	evaluate_attempt -> maybe_progress [style=solid]
	maybe_progress -> win [label="did_win=True" style=dashed]
	maybe_progress -> prompt_for_challenge [style=solid]
}
