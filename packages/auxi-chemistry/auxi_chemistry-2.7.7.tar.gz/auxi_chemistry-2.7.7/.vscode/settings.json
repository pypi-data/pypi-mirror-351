{
    "files.exclude": {
        ".mypy_cache": true,
        ".pytest_cache": true,
        ".ruff_cache": true,
        ".vscode": true,
        "build": true,
        "dist": true,
        "htmlcov": true,
        ".coverage": true,
        ".gitignore": true,
        ".gitlab-ci.yml": true,
        ".gitlab-ci-local": true,
        "Pipfile.lock": true,
        ".pre-commit-config.yaml": true,
        "typings": true,
        "uv.lock": true,
        ".venv": true,
        "**/__pycache__": true,
        "**/.directory": true,
        "**/*.egg-info": true
    },
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "cSpell.words": [
        "auxi",
        "brentq",
        "cmps",
        "endregion",
        "factps",
        "factsage",
        "fracs",
        "optimised",
        "Raphson",
        "thysn"
    ],
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "foreground": "#99e32b",
            "type": "text"
        },
        "FIXME": {
            "icon": "alert",
            "foreground": "#e3812b"
        },
        "QUESTION": {
            "icon": "question",
            "foreground": "#34deeb"
        }
    },
    "todo-tree.general.tags": [
        "TODO",
        "FIXME",
        "QUESTION"
    ]
}
